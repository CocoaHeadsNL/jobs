---
layout: nil
---

[
{% for post in site.posts %}
{
  "title" : "{{ post.title }}",
  "url" : "{{ post.url }}",
  "date" : "{{ post.date | date: date_to_rfc822 }}",
  "content" : "{{ post.content | escape }}",
  "tags" : [
    {% for tag in post.tags %}
      "{{ tag | escape }}"{% if forloop.last %}{% else %},{% endif %}
    {% endfor %}
  ]
} {% if forloop.last %}{% else %},{% endif %}
{% endfor %}
]